{
  "routes": [
    {
      "route": "/*",
      "serve": "/index.html",
      "statusCode": 200
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/images/*.{png,jpg,gif,webp}", "/css/*", "/js/*", "/assets/*"]
  },
  "mimeTypes": {
    ".json": "text/json",
    ".webp": "image/webp"
  }
}
